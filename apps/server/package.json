{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "license": "MIT",
  "scripts": {
    "copy-files": "copyfiles -u 1 \"src/openapi/**/*.yaml\" \"src/mailtrap/template/**\" dist/",
    "build": "pnpm clean && tsc && tsc-alias",
    "start:dev": "pnpm exec nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec tsx ./src/index.ts",
    "start:prod": "node dist/index",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-unknown --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-unknown --ignore-path ../../.prettierignore",
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist/",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.0"
  },
  "dependencies": {
    "@workspace/shared": "workspace:*",
    "chalk": "catalog:",
    "express": "catalog:",
    "morgan": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:",
    "@types/morgan": "catalog:",
    "@types/node": "catalog:",
    "@workspace/eslint": "workspace:*",
    "@workspace/typescript": "workspace:*",
    "copyfiles": "catalog:",
    "eslint": "catalog:",
    "nodemon": "catalog:",
    "rimraf": "catalog:",
    "tsc-alias": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
