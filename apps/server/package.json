{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "license": "MIT",
  "scripts": {
    "copy-files": "copyfiles -u 1 \"src/openapi/**/*.yaml\" \"src/mailtrap/template/**\" dist/",
    "apps:build": "pnpm clean && tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "start:dev": "cross-env NODE_ENV=development pnpm exec nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec tsx ./src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-unknown --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-unknown --ignore-path ../../.prettierignore",
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist/",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.2"
  },
  "dependencies": {
    "@getbrevo/brevo": "catalog:",
    "@workspace/shared": "workspace:*",
    "async-exit-hook": "catalog:",
    "bcryptjs": "catalog:",
    "chalk": "catalog:",
    "cloudinary": "catalog:",
    "cookie-parser": "catalog:",
    "cors": "catalog:",
    "cross-env": "catalog:",
    "dotenv": "catalog:",
    "express": "catalog:",
    "http-status-codes": "catalog:",
    "jsonwebtoken": "9.0.3",
    "lodash": "catalog:",
    "mongodb": "catalog:",
    "morgan": "catalog:",
    "ms": "catalog:",
    "multer": "catalog:",
    "socket.io": "catalog:",
    "streamifier": "catalog:",
    "uuid": "catalog:",
    "winston": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/async-exit-hook": "catalog:",
    "@types/cookie-parser": "catalog:",
    "@types/cors": "catalog:",
    "@types/express": "catalog:",
    "@types/jsonwebtoken": "catalog:",
    "@types/lodash": "catalog:",
    "@types/morgan": "catalog:",
    "@types/ms": "catalog:",
    "@types/multer": "catalog:",
    "@types/node": "catalog:",
    "@types/streamifier": "catalog:",
    "@workspace/eslint": "workspace:*",
    "@workspace/typescript": "workspace:*",
    "copyfiles": "catalog:",
    "eslint": "catalog:",
    "nodemon": "catalog:",
    "rimraf": "catalog:",
    "tsc-alias": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
