{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.28.1",
  "license": "MIT",
  "scripts": {
    "start:dev": "vite --host",
    "apps:build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:prod": "vite preview --host",
    "format": "prettier --check . --ignore-unknown --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-unknown --ignore-path ../../.prettierignore",
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist/",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.1"
  },
  "dependencies": {
    "@dnd-kit/core": "catalog:",
    "@dnd-kit/modifiers": "catalog:",
    "@dnd-kit/sortable": "catalog:",
    "@dnd-kit/utilities": "catalog:",
    "@emotion/react": "catalog:",
    "@emotion/styled": "catalog:",
    "@mui/icons-material": "catalog:",
    "@mui/lab": "catalog:",
    "@mui/material": "catalog:",
    "@reduxjs/toolkit": "catalog:",
    "@workspace/shared": "workspace:*",
    "@workspace/ui": "workspace:*",
    "axios": "catalog:",
    "lodash": "catalog:",
    "material-ui-confirm": "catalog:",
    "randomcolor": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "catalog:",
    "react-redux": "catalog:",
    "react-router-dom": "catalog:",
    "react-toastify": "11.0.5",
    "redux-persist": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/lodash": "catalog:",
    "@types/node": "catalog:",
    "@types/randomcolor": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "@workspace/eslint": "workspace:*",
    "@workspace/typescript": "workspace:*",
    "eslint": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-svgr": "catalog:"
  }
}
