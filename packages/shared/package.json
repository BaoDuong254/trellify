{
  "name": "@workspace/shared",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "license": "MIT",
  "scripts": {
    "pkg:build": "pnpm clean && tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check . --ignore-unknown --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-unknown --ignore-path ../../.prettierignore",
    "preinstall": "pnpm dlx only-allow pnpm",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist/"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.28.2"
  },
  "dependencies": {
    "chalk": "catalog:",
    "express": "catalog:",
    "http-status-codes": "catalog:",
    "rimraf": "catalog:",
    "winston": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@workspace/eslint": "workspace:*",
    "@workspace/typescript": "workspace:*",
    "eslint": "catalog:",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  },
  "exports": {
    "./utils/*": "./dist/utils/*.js",
    "./schemas/*": "./dist/schemas/*.js"
  }
}
